        </dt>

        <dd>
            @Html.DisplayFor(model => model.Assign_Course)
        </dd>

        <dt style="text-align:left">
            @Html.DisplayName("Email Id:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.EmailID)
        </dd>

        <dt style="text-align:left">
