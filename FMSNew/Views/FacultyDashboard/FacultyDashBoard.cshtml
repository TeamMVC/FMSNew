@model FMSNew.Models.GetFacultyData

@{
    ViewBag.Title = "FacultyDashBoard";
}

<h2>FacultyDashBoard</h2>
<div>

    <hr />
    <dl class="dl-horizontal">
        <dt style="text-align:left">
            @Html.DisplayName("Faculty Name :")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Faculty_Name)
        </dd>

        <dt style="text-align:left">
            @Html.DisplayName("Faculty Qualification:")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Faculty_Qualification)
        </dd>

        <dt style="text-align:left">
